<odoo>
    <record model="ir.ui.view" id="product_template_form_extend">
        <field name="name">Form view for product template Extend</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='barcode']" position="after">
                <field name="ref_product" string="Url SEO"/>
            </xpath>
        </field>
    </record>

</odoo>